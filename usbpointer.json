{
  "title": "Personal rules (@guoyida)",
  "rules": [
    {
      "description": "Change ENTER/F20 to left_option+p (pause/play media)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f20",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "p",
              "modifiers": [
                "left_option"
              ]
            },
            {
              "key_code": "vk_none"
            }
          ]
        }
      ]
    }
  ]
}